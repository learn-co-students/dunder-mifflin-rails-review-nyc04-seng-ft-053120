<%= form_for(@employee) do |f|%>
<p>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
</p>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
</p>
<p>
  <%= f.label :alias %>
  <%= f.text_field :alias %>
</p>
<p>
  <%= f.label :title %>
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :office %>
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :img_url %>
  <%= f.text_field :img_url %>
</p>
<p>
  <%= f.collection_radio_buttons :id, Dog.all, :id, :name %>
</p>
  <%= f.submit %>
<% end %>