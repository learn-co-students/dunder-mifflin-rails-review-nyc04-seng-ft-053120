<h1><%= @employee.first_name + " " + @employee.last_name %></h1>


<img src="<%= @employee.img_url %>">
<ul>
  <li><strong>Alias: </strong><%= @employee.alias %></li>
  <li><strong>Title: </strong><%= @employee.title %></li>
  <li><strong>Office: </strong><%= @employee.office %></li>
  <li><strong>Dog: </strong><%= link_to @employee.dog.name, dog_path(@employee.dog.id) %></li>
</ul>

<%= button_to "Edit Employee", edit_employee_path(@employee), method: :get %>
